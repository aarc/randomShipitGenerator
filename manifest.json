{
	"name": "Random Shipit Generator",
	"version": "0.0.8",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": [
				"libs/jQuery.min.js",
				"content.js"
			],
			"css": [
				"css/styles.css"
			]
		}
	],
	"icons": {
		"16": "images/16x16-normal.png",
		"48": "images/48x48-normal.png",
		"128": "images/128x128-normal.png"
	},
	"web_accessible_resources": ["images/xmas/38x38.png"],
	// https://developer.chrome.com/extensions/optionsV2
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	}
}
